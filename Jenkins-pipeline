#!/usr/bin/env groovy

pipeline{
agent none
stages{
stage('compile'){
steps{
script{
echo "Compiling the application ... "
}
}
}

stage('build'){
steps{
script{
echo "building the application ... "
}
}
}

stage('test'){
steps{
script{
echo "testing the application ... "
}
}
}

stage('deploy'){
steps{
script{
echo "deploying the application ... "
}
}
}
}
}
